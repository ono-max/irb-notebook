{
  "name": "irb-notebook",
  "displayName": "irb-notebook",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "keywords": [
    "notebookRenderer"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onNotebook:irb-notebook"
  ],
  "main": "./out/extension/extension.js",
  "browser": "./out/extension/extension.web.js",
  "contributes": {
    "notebookRenderer": [
      {
        "id": "irb-notebook",
        "entrypoint": "./out/client/index.js",
        "displayName": "irb-notebook",
        "mimeTypes": [
          "x-application/custom-json-output"
        ]
      }
    ],
    "notebooks": [
      {
        "type": "irb-notebook",
        "displayName": "irb Notebook",
        "selector": [
          {
            "filenamePattern": "*.irbnb"
          }
        ]
      }
    ],
    "commands": [
      {
        "title": "Debug cell",
        "command": "rdbg.debugCell",
        "icon": "$(bug)"
      }
    ],
    "menus": {
      "notebook/cell/execute": [
        {
          "command": "rdbg.debugCell",
          "when": "notebookCellEditable"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "webpack --mode development --watch",
    "pretest": "webpack --mode development && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "20.x",
    "@types/vscode": "^1.72.0",
    "@types/vscode-notebook-renderer": "^1.60.0",
    "@types/webpack-env": "^1.16.4",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vscode/test-electron": "^2.1.3",
    "css-loader": "^7.0.0",
    "eslint": "^8.14.0",
    "fork-ts-checker-webpack-plugin": "^9.0.0",
    "glob": "^11.0.0",
    "mocha": "^10.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.2.8",
    "typescript": "^5.0.0",
    "util": "^0.12.4",
    "vscode-notebook-error-overlay": "^1.0.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "@vscode/debugprotocol": "1.56.0",
    "buffer": "6.0.3",
    "path-browserify": "^1.0.1",
    "stream-http": "^3.2.0",
    "url": "^0.11.0"
  }
}
